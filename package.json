{
  "name": "vue-form-builder",
  "version": "1.0.0",
  "description": "A drag & drop form builder platform that made with Vue.js and Element UI and generate form sourcecode",
  "author": "GavinZhulei",
  "license": "LGPL-3.0-or-later",
  "keywords": [
    "component",
    "vue",
    "form",
    "sourcecode",
    "element-ui",
    "auto"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tentree-org/vue-form-builder.git"
  },
  "main": "dist/FormBuilder.common.js",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build && cp CNAME ./docs",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name FormBuilder ./src/index.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "axios": "^0.21.1",
    "brace": "^0.11.1",
    "core-js": "^3.23.3",
    "element-ui": "^2.15.2",
    "normalize.css": "^8.0.1",
    "qiniu": "^7.8.0",
    "qiniu-js": "^3.4.1",
    "sass": "^1.57.1",
    "sass-loader": "^10.1.1",
    "viewerjs": "^1.11.2",
    "vue": "^2.6.14",
    "vue-i18n": "^5.0.3",
    "vue-router": "^3.0.1",
    "vue-run-sfc": "^1.6.0",
    "vue2-editor": "^2.10.3",
    "vuedraggable": "^2.24.3"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/ace": "^0.0.48",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "babel-core": "^6.26.3",
    "rollup": "^2.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-uglify-es": "^0.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.44.2"
  },
  "bugs": {
    "url": "https://github.com/Paul-Brooker/vue-forms/issues"
  },
  "homepage": "https://github.com/Paul-Brooker/vue-forms#readme",
  "directories": {
    "doc": "docs"
  }
}
